   "source": [
    "N = int(input())\n",
    "\n",
    "if N == 1:\n",
    "    print(1)\n",
    "elif 1 < N <= 7:\n",
    "    print(2)\n",
    "else:\n",
    "    i = 1\n",
    "    while True:\n",
    "        i += 1\n",
    "        if 3*i*(i - 1) + 1 < N <= 3*(i + 1)*i + 1:\n",
    "            print(i + 1)\n",
    "            break"
   ]
  }

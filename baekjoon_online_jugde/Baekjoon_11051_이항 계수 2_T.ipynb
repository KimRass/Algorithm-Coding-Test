   "source": [
    "N, K = list(map(int, input().split()))\n",
    "\n",
    "res = 1\n",
    "minim = min(K, N - K)\n",
    "maxim = max(K, N - K)\n",
    "for i in range(N, maxim, -1):\n",
    "    res *= i\n",
    "for i in range(minim, 0, -1):\n",
    "    res //= i\n",
    "\n",
    "const = 10007\n",
    "print(res%const)"
   ]
  }

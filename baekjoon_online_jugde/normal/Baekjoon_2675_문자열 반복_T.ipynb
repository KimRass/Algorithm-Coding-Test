   "source": [
    "n = int(input())\n",
    "for _ in range(n):\n",
    "    R, str = input().split()\n",
    "    R = int(R)\n",
    "    res = \"\"\n",
    "    for char in str:\n",
    "        char_aug = char*R\n",
    "        res += char_aug\n",
    "    print(res)"
   ]
  }

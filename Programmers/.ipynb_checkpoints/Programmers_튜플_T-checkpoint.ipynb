    "def solution(s):\n",
    "    s = f\"[{s[1:-1]}]\"\n",
    "    s = eval(s)\n",
    "    s = sorted(s, key=lambda x:len(x))\n",
    "    ans = [list(j - i)[0] for i, j in zip(s, s[1:])]\n",
    "    ans = [list(s[0])[0]] + ans\n",
    "    return ans"
   ]

    "def solution(n):\n",
    "    one = bin(n).count(\"1\")\n",
    "    \n",
    "    tar = n + 1\n",
    "    while True:\n",
    "        if bin(tar).count(\"1\") == one:\n",
    "            return tar\n",
    "        else:\n",
    "            tar += 1"
   ]

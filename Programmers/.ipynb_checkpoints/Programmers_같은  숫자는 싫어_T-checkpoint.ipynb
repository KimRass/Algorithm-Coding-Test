   "source": [
    "def solution(arr):\n",
    "    stack = [\"FAKE\"]\n",
    "    ans = list()\n",
    "    for num in arr:\n",
    "        if num != stack[-1]:\n",
    "            ans.append(num)\n",
    "            stack.append(num)\n",
    "        else:\n",
    "            continue\n",
    "    return ans"
   ]

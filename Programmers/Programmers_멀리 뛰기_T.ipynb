    "import math\n",
    "\n",
    "def solution(n):\n",
    "    tar = 1234567\n",
    "    ans = 0\n",
    "    for k in range(n//2 + 1):\n",
    "        sub_ans = math.comb(n - k, k)%tar\n",
    "        ans += sub_ans\n",
    "    return ans%tar"
   ]

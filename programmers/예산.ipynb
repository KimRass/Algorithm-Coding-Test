   "source": [
    "def solution(d, budget):\n",
    "    d = sorted(d)\n",
    "    cnt = 0\n",
    "    s = 0\n",
    "    for m in d:\n",
    "        s += m\n",
    "        if s <= budget:\n",
    "            cnt += 1\n",
    "        else:\n",
    "            break\n",
    "    return cnt"
   ]

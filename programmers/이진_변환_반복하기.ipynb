    "def solution(s):\n",
    "    ans = [0, 0]\n",
    "    while True:\n",
    "        delete = s.count(\"0\")\n",
    "        s = bin(s.count(\"1\"))[2:]\n",
    "        ans[0] += 1\n",
    "        ans[1] += delete\n",
    "        if s == \"1\":\n",
    "            break\n",
    "    return ans"
   ]
